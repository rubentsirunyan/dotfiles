  cycle_prev = <Alt>bracketleft
  go_down = <Shift><Alt>j
  go_left = <Shift><Alt>h
  go_next = <Shift><Alt>braceright
  go_prev = <Shift><Alt>braceleft
  go_right = <Shift><Alt>l
  go_up = <Shift><Alt>k
  next_tab = <Primary>Tab
  prev_tab = <Primary><Shift>Tab
  split_horiz = <Alt>h
  split_vert = <Alt>v
